@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_DistortionPlus_#_DistortionPlus_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxdistortionplus.html> ;
        modgui:stylesheet <modgui/stylesheet-gxdistortionplus.css> ;
        modgui:screenshot <modgui/screenshot-gxdistortionplus.png> ;
        modgui:thumbnail <modgui/thumbnail-gxdistortionplus.png> ;
        modgui:brand "Guitarix " ;
        modgui:label "DistortionPlus" ;
        modgui:model "boxy" ;
        modgui:panel "3-knobs" ;
        modgui:color "mxr" ;
        modgui:knob "cairo" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "OUTPUT" ;
            lv2:name "OUTPUT" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "DISTORTION" ;
            lv2:name "DISTORTION" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "BASSBOOST" ;
            lv2:name "BASSBOOST" ;
        ] ;
    ] .
